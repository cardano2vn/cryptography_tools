
{
  "version": 2,
  "routes": [
    {
      "src": "/rsa",
      "dest": "/index.html"
    },
    {
      "src": "/symmetric", 
      "dest": "/symmetric.html"
    },
    {
      "src": "/hash",
      "dest": "/hash.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/symmetric.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.21"
    }
  }
}